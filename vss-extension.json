{
    "manifestVersion": 1,
    "id": "SpectralLint",
    "name": "Spectral Lint",
    "version": "0.11.0",
    "public": true,
    "publisher": "erwinkramer",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "tags": [
        "spectral",
        "api",
        "openapi",
        "owasp",
        "linting",
        "linter",
        "lint",
        "api management"
    ],
    "description": "Linter for OpenAPI definitions.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "branding": {
        "color": "#6F42C1",
        "theme": "dark"
    },
    "links": {
        "support": {
            "uri": "https://github.com/erwinkramer/ado-spectral/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/erwinkramer/ado-spectral"
    },
    "files": [
        {
            "path": "tasks"
        },
        {
            "path": "images",
            "addressable": true
        }
    ],
    "content": {
        "details": {
            "path": "OVERVIEW.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks"
            }
        }
    ]
}